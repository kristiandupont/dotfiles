// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "cmd+t cmd+f",
        "command": "tslint.fixAllProblems"
    },
    {
      "key": "ctrl+b",
      "command": "-extension.vim_ctrl+b",
      "when": "editorTextFocus && vim.active && vim.use<C-b> && !inDebugRepl && vim.mode != 'Insert'"
    },
    {
      "key": "ctrl+j",
      "command": "-extension.vim_ctrl+j",
      "when": "editorTextFocus && vim.active && vim.use<C-j> && !inDebugRepl && vim.mode != 'Insert'"
    },
    {
      "key": "alt+k",
      "command": "editor.action.insertLineBefore",
      "when": "editorTextFocus && !editorReadonly"
    },
    {
      "key": "ctrl+shift+enter",
      "command": "-editor.action.insertLineBefore",
      "when": "editorTextFocus && !editorReadonly"
    },
    {
      "key": "alt+j",
      "command": "editor.action.insertLineAfter",
      "when": "editorTextFocus && !editorReadonly"
    },
    {
      "key": "ctrl+enter",
      "command": "-editor.action.insertLineAfter",
      "when": "editorTextFocus && !editorReadonly"
    },
    {
      "key": "ctrl+tab",
      "command": "workbench.action.terminal.focusNext",
      "when": "terminalFocus"
    },
    {
      "key": "ctrl+shift+tab",
      "command": "workbench.action.terminal.focusPrevious",
      "when": "terminalFocus"
    },
    {
      "key": "ctrl+f",
      "command": "-extension.vim_ctrl+f",
      "when": "editorTextFocus && vim.active && vim.use<C-f> && !inDebugRepl"
    },
    {
      "key": "cmd+[Semicolon]",
      "command": "workbench.action.terminal.toggleTerminal"
    },
    {
      "key": "ctrl+shift+[Equal]",
      "command": "-workbench.action.terminal.toggleTerminal"
    },
    {
      "key": "shift+cmd+[Semicolon]",
      "command": "workbench.action.terminal.new"
    },
    {
      "key": "shift+alt+7",
      "command": "-gitlens.showCommitSearch",
      "when": "gitlens:enabled"
    },
    {
      "key": "ctrl+a",
      "command": "-extension.vim_ctrl+a",
      "when": "editorTextFocus && vim.active && vim.use<C-a> && !inDebugRepl"
    },
    {
      "key": "ctrl+n",
      "command": "-extension.vim_ctrl+n",
      "when": "editorTextFocus && vim.active && vim.use<C-n> && !inDebugRepl"
    },
    {
      "key": "ctrl+f4",
      "command": "-extension.node-debug.pickLoadedScript",
      "when": "debugType == 'chrome'"
    },
    {
      "key": "ctrl+f4",
      "command": "-extension.node-debug.pickLoadedScript",
      "when": "debugType == 'node'"
    },
    {
      "key": "ctrl+f4",
      "command": "-extension.node-debug.pickLoadedScript",
      "when": "debugType == 'node2'"
    },
    {
      "key": "f2",
      "command": "renameFile",
      "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
    },
    {
      "key": "enter",
      "command": "-renameFile",
      "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
    },
    {
      "key": "enter",
      "command": "-renameFile",
      "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !inputFocus"
    },
    {
      "key": "ctrl+k",
      "command": "workbench.action.terminal.clear"
    }
  ]
