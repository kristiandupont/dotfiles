// Place your settings in this file to overwrite the default settings
{
  "editor.detectIndentation": false,
  "editor.fontSize": 10.5,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "extensions.autoUpdate": true,
  "extensions.ignoreRecommendations": false,
  "files.associations": {
    "*.erb": "markdown",
    "*.mdconf": "markdown",
    "*.har": "json"
  },
  "files.insertFinalNewline": true,
  "gitlens.advanced.messages": {
    "suppressFileNotUnderSourceControlWarning": true,
    "suppressShowKeyBindingsNotice": true,
    "suppressResultsExplorerNotice": true,
    "suppressWelcomeNotice": true
  },
  "gitlens.codeLens.enabled": false,
  "json.schemas": [
    {
      "fileMatch": ["*.har"],
      "url": "https://raw.githubusercontent.com/jarib/har/master/schema.json"
    }
  ],
  "search.useIgnoreFiles": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "terminal.integrated.shell.linux": "/usr/bin/fish",
  "vim.enableNeovim": true,
  "vim.foldfix": true,
  "vim.neovimPath": "C:\\Program Files\\Neovim\\bin\\nvim.exe",
  "vim.easymotion": true,
  "vim.leader": "<space>",
  "vim.useCtrlKeys": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["tab"],
      "after": [],
      "commands": [
        {
          "command": "tabout",
          "args": []
        }
      ]
    },
    {
      "before": ["leader", "m"],
      "after": [],
      "commands": [
        {
          "command": "textmarker.toggleHighlight",
          "args": []
        }
      ]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["C-v"],
      "after": [],
      "commands": "editor.action.clipboardPasteAction"
    }
  ],
  "vim.handleKeys": {
    "<C-c>": false,
    "<C-v>": false,
    "<C-f>": false,
    "<C-x>": false,
    "<C-j>": false
  },
  "vim.useSystemClipboard": false,
  "window.openFilesInNewWindow": "off",
  "workbench.editor.enablePreview": false,
  "workbench.editor.showTabs": false,
  "workbench.iconTheme": "vs-minimal",
  "workbench.sideBar.location": "right",
  "vim.disableExtension": false,
  "typescript.tsserver.log": "normal",
  "gitlens.keymap": "chorded",
  "breadcrumbs.enabled": true,
  "window.titleBarStyle": "custom",
  "window.restoreWindows": "none",
  "textmarker.defaultHighlightColor": "#fce00c",
  "textmarker.defaultHighlightOpacity": 1,
  "textmarker.highlightColors": [
    "#fc800c",
    "darkmagenta",
    "darkolivegreen",
    "darkslateblue",
    "darkslategray",
    "darkviolet",
    "darkblue",
    "darkturquoise",
    "darkgray",
    "darkkhaki",
    "darkorange",
    "darksalmon",
    "darkseagreen"
  ],
  "diffEditor.renderSideBySide": true,
  "gitlens.views.fileHistory.enabled": true,
  "gitlens.views.lineHistory.enabled": true,
  "workbench.colorTheme": "Palenight Operator",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.preferredDarkColorTheme": "Palenight Operator",
  "window.autoDetectColorScheme": true,
  "remote.SSH.remotePlatform": {
    "C02T10EXH040-by-ip": "linux",
    "local-ubuntu": "linux",
    "M1-Ubuntu": "linux",
    "M1-Ubuntu-by-ip": "linux"
  },
  "yaml.schemas": {
    "file:///home/kristian/.vscode-server/extensions/atlassian.atlascode-2.8.5/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "terminal.integrated.automationShell.windows": "",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[rust]": {
    "editor.defaultFormatter": "matklad.rust-analyzer"
  },
  "[shellscript]": {
    "files.eol": "\n"
  },
  "gitlens.views.branches.showBranchComparison": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "slides.previewMarkdownFiles": true,
  "git.autofetch": true
}
